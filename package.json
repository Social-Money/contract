{
  "name": "social-money-contract",
  "version": "1.0.0",
  "description": "social money contract",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "truffle compile",
    "migrate": "truffle migrate --network rinkeby",
